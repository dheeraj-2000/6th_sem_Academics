print(m)
seed <- m
print("ok1")
list1 = ic(g,seed)
print("ok2")
len <- length(list1)
print("ok3")
inspow <- c(inspow,len)
print(inspow)
print("ok4")
}
pow <- c(pow ,mean(inspow))
print(pow)
}
