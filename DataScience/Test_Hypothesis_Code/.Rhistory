ic <- function(g,s){
jst_inf <- c(s)
infected <- c(s)
while(1){
if (length(jst_inf)==0)
return(infected)
temp <- c()
for(i in jst_inf){
for (j in neighbors(g,i)) {
r= runif(1)
if (r<0.5 && (!j %in% infected) && (!j %in% temp)){
temp <- c(temp,j)
}
}
}
for (k in temp){
infected <- c(infected,k)
}
jst_inf <- temp
}
}
s <- c()
pow <- c()
#yahan 1 ki jagah 16 kardena
